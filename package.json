{
  "name": "skylark-ui-gradienter",
  "description": "The skylark gradient picker widget",
  "version": "0.9.0",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "https://github.com/skylarkui/skylark-ui-gradienter/",
  "author": "Hudaokeji, Inc.",
  "scripts": {
    "build": "sbundle build .",
    "change-version": "node grunt/change-version.js",
    "update-shrinkwrap": "npm shrinkwrap --dev && shx mv ./npm-shrinkwrap.json ./grunt/npm-shrinkwrap.json",
    "test": "grunt test"
  },
  "style": "dist/css/bootstrap.css",
  "less": "less/bootstrap.less",
  "main": "./dist/js/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/skylarkui/skylark-ui-window.git"
  },
  "bugs": {
    "url": "https://github.com/skylarkui/skylark-bs-window/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "skylark-langx": "https://github.com/skylarklangx/skylark-langx#master",
    "skylark-utils": "https://github.com/skylarkutils/skylark-utils#master",
    "skylark-utils-color": "https://github.com/skylarkutils/skylark-utils-color#master",
    "skylark-utils-interact": "https://github.com/skylarkutils/skylark-utils-interact#master",
    "skylark-ui-swt": "https://github.com/skylarkui/skylark-ui-swt#master",
    "skylark-ui-colorpicker": "https://github.com/skylarkui/skylark-ui-colorpicker#master",
    "skylark-utils-dom": "https://github.com/skylarkutils/skylark-utils-dom#master"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "dist",
    "fonts",
    "grunt",
    "js/*.js",
    "less/**/*.less",
    "Gruntfile.js",
    "LICENSE"
  ],
  "jspm": {
    "main": "js/bootstrap",
    "shim": {
      "js/bootstrap": {
        "deps": "jquery",
        "exports": "$"
      }
    },
    "files": [
      "css",
      "fonts",
      "js"
    ]
  }
}
